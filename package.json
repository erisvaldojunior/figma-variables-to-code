{
  "dependencies": {
    "@create-figma-plugin/ui": "^3.2.0",
    "@create-figma-plugin/utilities": "^3.2.0",
    "jszip": "^3.10.1",
    "preact": ">=10",
    "shiki": "^1.14.1"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^3.2.0",
    "@create-figma-plugin/tsconfig": "^3.2.0",
    "@figma/plugin-typings": "1.90.0",
    "@types/node": "^24.0.9",
    "concurrently": ">=8",
    "tailwindcss": ">=3",
    "typescript": ">=4"
  },
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "tailwindcss --input ./src/css/input.css --output ./src/css/output.css",
    "build:js": "build-figma-plugin --typecheck --minify",
    "watch": "npm run build:css && concurrently npm:watch:css npm:watch:js",
    "watch:css": "tailwindcss --input ./src/css/input.css --output ./src/css/output.css --watch",
    "watch:js": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "capabilities": [
      "inspect"
    ],
    "editorType": [
      "figma",
      "dev"
    ],
    "id": "1410237134114371338",
    "name": "Figma Variables to Code",
    "networkAccess": {
      "allowedDomains": [
        "https://api.github.com"
      ]
    },
    "main": "src/main.ts",
    "ui": "src/ui/Plugin.tsx"
  }
}
